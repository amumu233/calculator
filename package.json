{
  "name": "calculator",
  "version": "1.0.0",
  "description": "基于electron和javascript的桌面计算器应用",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build:win": "electron-packager ./ 计算器 --platform=win32 --overwrite --icon=./static/icon.ico",
    "dist": "electron-builder --win"
  },
  "build": {
    "appId": "com.amumu233.calculator",
    "productName": "计算器",
